<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog title="Details of last three positions (Excluding Present)">
		<VBox class="sapUiMediumMargin">
			<CheckBox id="_IDGenCheckBoxAgree1" select="onAgreeSelectionChanged" text="I agree to save the self appraisal for the approval"
				selected="false"/>
		</VBox>
		<footer>
			<OverflowToolbar>
				<ToolbarSpacer/>
				<Button id="cancel" text="Cancel" press="handleCancelAppraisalPress"/>
				<Button id="agree" text="Save Appraisal" press="handleSaveAppraisalPress" enabled="false"/>
			</OverflowToolbar>
		</footer>
	</Dialog>
</core:FragmentDefinition>