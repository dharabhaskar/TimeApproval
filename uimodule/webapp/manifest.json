{
	"_version": "1.21.0",
	"sap.app": {
		"id": "com.infocus.TimeApprovalV2",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"crossNavigation": {
			"inbounds": {
				"intent1": {
					"signature": {
						"parameters": {},
						"additionalParameters": "allowed"
					},
					"semanticObject": "uimodule",
					"action": "display",
					"title": "Time Approval - V2",
					"icon": "sap-icon://add"
				}
			}
		},
		"dataSources": {
			"ZTIMESHEET_MAN_SRV": {
				"uri": "/sap/opu/odata/sap/ZTIMESHEET_MAN_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "model/metadata.xml"
				}
			}
		},
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": true,
		"rootView": {
			"viewName": "com.infocus.TimeApprovalV2.view.MainView",
			"type": "XML",
			"async": true,
			"id": "idAppControl"
		},
		"dependencies": {
			"minUI5Version": "1.60.0",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.infocus.TimeApproval.i18n.i18n"
				}
			},
			"": {
				"uri": "/sap/opu/odata/sap/ZTIMESHEET_MAN_SRV/",
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBuindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "ZTIMESHEET_MAN_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.infocus.TimeApproval.view",
				"controlId": "idAppControl",
				"controlAggregation": "pages",
				"async": true
			},
			"routes": [
				{
					"name": "RouteMainView",
					"pattern": "RouteMainView",
					"target": [
						"TargetMainView"
					]
				}
			],
			"targets": {
				"TargetMainView": {
					"viewType": "XML",
					"viewLevel": 1,
					"viewId": "idAppControl",
					"viewName": "MainView"
				}
			}
		}
	},
	"sap.cloud": {
		"service": "TimeApproval.service"
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/ztms_man/uimodule/webapp",
		"_version": "1.1.0"
	}
}